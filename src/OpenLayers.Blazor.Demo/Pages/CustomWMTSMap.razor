@page "/customdemo"

<h3>CustomWMTSMap</h3>

<div class="container">
    <Map Style="height:800px" Zoom="5" Center="new Coordinate(40.72, -73)" CoordinatesProjection="EPSG:4326">
        <Layers>
            <Layer SourceType="SourceType.XYZ" Url="@("https://basemap.nationalmap.gov/arcgis/rest/services/USGSTopo/MapServer/tile/{z}/{y}/{x}")" CrossOrigin="anonymous" Format="image/png"></Layer>
        </Layers>
    </Map>
</div>

